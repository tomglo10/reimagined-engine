     <b:widget-setting name='content'><![CDATA[<style>#random-posts img{background-color:#F5F5F5;float:left;height:75px;margin-right:10px;padding:3px;width:100px}ul#random-posts{list-style-type:none}#random-posts li{margin-bottom:10px}.random-summary{display:block}</style>
<ul id='random-posts'><script type='text/javaScript'>var randomposts_number = 5;var randomposts_chars = 60;var randomposts_details = 'yes';randomposts_details2 = 'no';var randomposts_comments = 'Comments';var randomposts_commentsd = 'Comments Disabled';var randomposts_current = [];var total_randomposts = 0;var randomposts_current = new Array(randomposts_number);
function randomposts(t){total_randomposts=t.feed.openSearch$totalResults.$t}function getvalue(){for(var t=0;t<randomposts_number;t++){for(var r=!1,o=get_random(),s=0;s<randomposts_current.length;s++)if(randomposts_current[s]==o){r=!0;break}r?t--:randomposts_current[t]=o}}function get_random(){return 1+Math.round(Math.random()*(total_randomposts-1))}document.write('<script type="text/javascript" src="/feeds/posts/default?alt=json-in-script&max-results=0&callback=randomposts"><\/script>');</script>
<script type='text/javaScript'>function random_posts(t){for(var s=0;s<randomposts_number;s++){var e=t.feed.entry[s],r=e.title.$t;if("content"in e)var n=e.content.$t;else if("summary"in e)n=e.summary.$t;else n="";if((n=n.replace(/<[^>]*>/g,"")).length<randomposts_chars)var a=n;else{var i=(n=n.substring(0,randomposts_chars)).lastIndexOf(" ");a=n.substring(0,i)+"&#133;"}for(var o=0;o<e.link.length;o++){if("thr$total"in e)var m=e.thr$total.$t+" "+randomposts_comments;else m=randomposts_commentsd;if("alternate"==e.link[o].rel){var l=e.link[o].href,d=e.published.$t;if("media$thumbnail"in e)var c=e.media$thumbnail.url;else c="https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEhzEwCXFp0mZdMngrTXvXKegcFSJU0RYGh0RzM3Ty0q-QZGAhH3cXe5dtbwV0-izVpqpIBMiAB92Ctof_4Ifj3G6LtOu7E3n_4Scr-WPgJyk28P4mxs7_nxjx-7J75Maci780NwKSq7CLA/s1600/no_thumb.png"}}document.write("<li>"),document.write('<img alt="'+r+'" src="'+c+'"/>'),document.write('<div><a href="'+l+'" rel="nofollow">'+r+"</a></div>"),"yes"==randomposts_details&&document.write("<span>"+d.substring(8,10)+"."+d.substring(5,7)+"."+d.substring(0,4)+" - "+m),"yes"==randomposts_details2&&document.write('<span class="random-summary">'+a),document.write('<div style="clear:both"></div></li>')}}getvalue();for(var i=0;i<randomposts_number;i++)document.write('<script type="text/javascript" src="/feeds/posts/default?alt=json-in-script&start-index='+randomposts_current[i]+'&max-results=1&callback=random_posts"><\/script>');</script></ul><div style="font-size: 10px; float: right;"><a href="http://shoutblogloud.blogspot.com/2016/01/add-random-posts-widget-to-blogger.html" rel="nofollow">Random Posts Widget</a></div>]]></b:widget-setting>

